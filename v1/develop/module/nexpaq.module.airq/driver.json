{
    "type": "nexpaq.module.airq",
    "version": "1.0.0",
    "commands": [
        {
            "name": "StartSensor",
            "title": "Start AirQ sensor",
            "description": "Start AirQ sensor to read data",
            "command": "2700",
            "data": [
                {
                    "defaultValue": 3
                }
            ]
        },
        {
            "name": "StopSensor",
            "title": "Stop AirQ sensor",
            "description": "Stop reading data from AirQ module.",
            "command": "2700",
            "data": [
                {
                    "defaultValue": 4
                }
            ]
        }
    ],
    "dataVariables": [
        {
            "name": "state",
            "title": "State",
            "description": "Notifies state of the sensor",
            "data": [0, 1],
            "state": {
                "FFFF": "heating",
                "FF00": "updating reference"
            },
            "source": "data"
        },
        {
            "name": "adc",
            "title": "ADC value",
            "description": "ADC value detected by module",
            "data": [0, 1],
            "format": "({0} * 256 + {1})",
            "source": "data"
        }
    ]
}